    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "eosjs": "^21.0.3",
    "express": "^4.16.4",
    "express-handlebars": "^5.2.0",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2",
    "socket.io": "^3.1.1",
    "socket.io-client": "^3.1.1"
  },
  "preferGlobal": true,
