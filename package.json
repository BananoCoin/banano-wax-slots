{
  "name": "wax-slots",
  "version": "1.7.1",
  "description": "wax-slots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananoCoin/banano-wax-slots.git"
  },
  "scripts": {
    "eslint": "eslint --fix 'scripts/**/*.js'",
    "jsonlint": "jsonlint -q package.json;jsonlint -q scripts;jsonlint -q config.json;",
    "new-config": "node scripts/new-config.js",
    "npm-check-updates": "npm-check-updates --target minor -u",
    "preflight": "npm update;npm run npm-check-updates;npm i;npm audit fix;npm outdated;npm run eslint;npm run jsonlint;",
    "start": "node -max-old-space-size=1700 --trace-warnings scripts/index.js",
    "screenstart": "screen -dmSL banano_wax_slots npm start;screen -x banano_wax_slots",
    "screenrestart": "npm stop;screen -x banano_wax_slots;screen -dmSL banano_wax_slots npm start;screen -x banano_wax_slots",
    "stop": "node scripts/stop.js"
  },
  "author": "Coranos",
  "license": "MIT",
  "dependencies": {
    "@bananocoin/bananojs": "^2.5.2",
    "abort-controller": "^3.0.0",
    "await-semaphore": "^0.1.3",
    "blakejs": "^1.1.1",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "eosjs": "^22.1.0",
    "express": "^4.17.2",
    "express-handlebars": "^6.0.2",
    "node-fetch": "^2.6.7",
    "request": "^2.88.2",
    "sharp": "^0.30.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@prantlf/jsonlint": "^10.2.0",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.2.0",
    "npm-check-updates": "^12.3.0",
    "nyc": "^15.1.0",
    "xml2json": "^0.12.0"
  }
}
